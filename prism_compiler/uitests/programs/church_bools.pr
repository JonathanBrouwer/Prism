let Bool = (T: Type) -> T -> T -> T;
let true = (T: Type) => (v1: T) => (v2: T) => v1; // true returns v1
let false = (T: Type) => (v1: T) => (v2: T) => v2; // false returns v2

// and : Bool -> Bool -> Bool
let and = (b1: Bool) => (b2: Bool) => b1 Bool b2 false;

// or : Bool -> Bool -> Bool
let or = (b1: Bool) => (b2: Bool) => b1 Bool true b2;

// example
and true true

### Eval
(T: Type) => (v1: T) => (_: T) => v1

### Type
(T: Type) -> T -> T -> T

