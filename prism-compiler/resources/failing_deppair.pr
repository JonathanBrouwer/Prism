let Pair
    : (T1: Type) (T2 : (T1 -> Type)) -> Type
    = (T1: Type) => (T2 : (T1 -> Type)) => (Out: Type) -> (destruct: ((v1: T1) -> (v2: T2 v1) -> Out)) -> Out;
let snd
    : (T1: Type) (T2 : (T1 -> Type)) -> (p: Pair T1 T2) -> T2 (fst T1 T2 p)
    = (T1: Type) (T2: T1 -> Type) (p: Pair T1 T2) => p (T2 (fst T1 T2 p)) ((v1: T1) (v2: T2 v1) => v2);
snd